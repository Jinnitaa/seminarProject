<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.seminarproject.welcomeController">
   <children>
      <BorderPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #F6F6FF;">
         <left>
            <AnchorPane prefHeight="200.0" prefWidth="200.0" style="-fx-background-color: #F6F6FF;" BorderPane.alignment="CENTER">
               <children>
                  <Label fx:id="welcome" layoutX="120.0" layoutY="39.0" text="Welcome " textFill="#152b55">
                     <font>
                        <Font name="Britannic Bold" size="64.0" />
                     </font>
                  </Label>
                  <Label fx:id="username" layoutX="225.0" layoutY="150.0" textFill="#152b55">
                     <font>
                        <Font name="Britannic Bold" size="64.0" />
                     </font>
                  </Label>
                  <Button fx:id="continueButton" layoutX="336.0" layoutY="336.0" mnemonicParsing="false" onAction="#continueSetOnAction" prefHeight="30.0" prefWidth="85.0" style="-fx-border-color: #152b55; -fx-background-color: #F6F6FF;" text="Continue ">
                     <font>
                        <Font name="Constantia Bold" size="14.0" />
                     </font>
                  </Button>
                  <Button fx:id="logoutButton" layoutX="158.0" layoutY="336.0" mnemonicParsing="false" onAction="#logoutButtonSetOnAction" prefHeight="30.0" prefWidth="85.0" style="-fx-border-color: #152b55; -fx-background-color: #F6F6FF;" text="LogOut ">
                     <font>
                        <Font name="Constantia Bold" size="14.0" />
                     </font>
                  </Button>
                  <ImageView fx:id="seminar" fitHeight="185.0" fitWidth="271.0" layoutX="158.0" layoutY="125.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@seminar.jpg" />
                     </image>
                  </ImageView>
               </children>
            </AnchorPane>
         </left>
      </BorderPane>
   </children>
</AnchorPane>
