            <Font name="Constantia" size="18.0" />
         </font>
      </Button>
      <Button layoutX="338.0" layoutY="257.0" mnemonicParsing="false" onAction="#deleteButtonSetOnAction" prefHeight="49.0" prefWidth="137.0" style="-fx-background-color: #152b55;" text="Delete" textFill="WHITE">
