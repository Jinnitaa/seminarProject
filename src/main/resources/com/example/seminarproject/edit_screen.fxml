                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </TextField>
                  <TextField fx:id="date" layoutX="23.0" layoutY="140.0" opacity="0.88" prefHeight="28.0" prefWidth="332.0" style="-fx-border-color: #152b55; -fx-background-color: #F6F6FF;">
