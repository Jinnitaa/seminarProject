                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </TextField>
                  <TextField fx:id="guest" layoutX="23.0" layoutY="86.0" opacity="0.88" prefHeight="28.0" prefWidth="333.0" style="-fx-border-color: #152b55; -fx-background-color: #F6F6FF;">
